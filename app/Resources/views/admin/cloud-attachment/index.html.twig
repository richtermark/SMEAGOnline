{% extends 'admin/cloud-file/layout.html.twig' %}

{% set menu = 'admin_cloud_attachment'%}
{% do load_script('cloudfile/manage') %}

{% block stylesheets %}
  {{parent()}}
  <style>
  .batch-item {
      position: absolute;
      left: 6px;
      top: 3px;
      z-index: 101;
  }
</style>
{% endblock %}

{% block main %}
<div class="eduyun-main">
  <form class="form-inline es-materials-manage" id="materials-form">
    {% include 'admin/cloud-attachment/form/type.html.twig' %}
    {% include 'admin/cloud-attachment/form/nav.html.twig' %}
    {% include 'admin/cloud-file/form/filter.html.twig' with {'pageType': 'attachment'} %}
    {% include 'admin/cloud-file/form/sort.html.twig' %}
    {% include 'admin/cloud-attachment/form/batch.html.twig' %}
    {% include 'admin/cloud-file/table.html.twig' with {'pageType': 'attachment'} %}
    <input type="hidden">
  </form>
</div>
{% endblock %}