{% extends 'admin/operation-analysis/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/paid-lesson' %}
{% set href="admin_operation_analysis_course_paid" %}
{% set menu = 'paid-lesson' %}
{% block analysisBody %}
  <div class="col-md-12">

    {% if tab=="trend" %}
      <div id="line-data"></div>
      <input id="data" type="hidden" value='{% if data %}{{ data }}{% endif %}'>
      <input id="paidCourseStartDate" type="hidden" value='{% if paidCourseStartDate %}{{ paidCourseStartDate }}{% endif %}'>
    {% elseif tab=="detail" %}
      {% include 'admin/operation-analysis/paid-course.table.html.twig' %}
    {% endif %}
  </div>
{% endblock %}
