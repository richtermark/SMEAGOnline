{% extends 'admin/operation-analysis/analysis-base-layout.html.twig' %}
{% set script_controller = 'analysis/lesson' %}
{% set href="admin_operation_analysis_task" %}
{% set menu = 'task' %}
{% block analysisBody %}
  <div class="col-md-12">

    {% if tab=="trend" %}
      <div id="line-data"></div>
      <input id="data" type="hidden" value='{% if data %}{{ data }}{% endif %}'>
      <input id="taskStartDate" type="hidden" value='{% if taskStartDate %}{{ taskStartDate }}{% endif %}'>
    {% elseif tab=="detail" %}
      {% include 'admin/operation-analysis/task.table.html.twig' %}
    {% endif %}
  </div>
{% endblock %}
